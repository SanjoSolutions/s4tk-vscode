{
	"name": "cat-customs",
	"displayName": "Cat Customs",
	"description": "Custom Editor API Samples",
	"version": "0.1.0",
	"private": true,
	"license": "MIT",
	"author": "frankkulak",
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {
		"@s4tk/models": "^0.6.6"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.73.0",
		"tsc-alias": "^1.8.4",
		"typescript": "^5.0.2"
	},
	"engines": {
		"vscode": "^1.76.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"contributes": {
		"customEditors": [
			{
				"viewType": "catCustoms.catScratch",
				"displayName": "Cat Scratch",
				"selector": [
					{
						"filenamePattern": "*.cscratch"
					}
				]
			},
			{
				"viewType": "catCustoms.pawDraw",
				"displayName": "Paw Draw",
				"selector": [
					{
						"filenamePattern": "*.pawdraw"
					}
				]
			},
			{
				"viewType": "catCustoms.stringTable",
				"displayName": "String Table",
				"selector": [
					{
						"filenamePattern": "*.stbl"
					}
				]
			}
		],
		"commands": [
			{
				"command": "catCustoms.pawDraw.new",
				"title": "Create new Paw Draw Document",
				"category": "Paw Draw"
			}
		]
	}
}
{
	"name": "s4tk-vscode",
	"displayName": "Sims 4 Toolkit",
	"description": "Sims 4 Toolkit support for VS Code.",
	"version": "0.1.0",
	"private": true,
	"license": "MIT",
	"author": "frankkulak",
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {
		"@s4tk/hashing": "^0.2.1",
		"@s4tk/models": "^0.6.6",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/uuid": "^9.0.1",
		"@types/vscode": "^1.73.0",
		"tsc-alias": "^1.8.4",
		"typescript": "^5.0.2"
	},
	"engines": {
		"vscode": "^1.76.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"contributes": {
		"commands": [
			{
				"command": "s4tk.hashing.fnv31",
				"title": "Hash text with FNV31",
				"category": "FNV Hashing"
			},
			{
				"command": "s4tk.hashing.fnv32",
				"title": "Hash text with FNV32",
				"category": "FNV Hashing"
			},
			{
				"command": "s4tk.hashing.fnv64",
				"title": "Hash text with FNV64",
				"category": "FNV Hashing"
			},
			{
				"command": "s4tk.hashing.random32",
				"title": "Generate random FNV32 hash",
				"category": "FNV Hashing"
			},
			{
				"command": "s4tk.hashing.random64",
				"title": "Generate random FNV64 hash",
				"category": "FNV Hashing"
			},
			{
				"command": "s4tk.fileCreate.stblBinary",
				"title": "Create empty STBL",
				"category": "TS4 Files"
			},
			{
				"command": "s4tk.fileCreate.stblJson",
				"title": "Create empty STBL JSON",
				"category": "TS4 Files"
			}
		],
		"customEditors": [
			{
				"viewType": "s4tk.editor.stblBinary",
				"priority": "default",
				"displayName": "String Table",
				"selector": [
					{
						"filenamePattern": "*.stbl"
					}
				]
			}
		],
		"jsonValidation": [
			{
				"fileMatch": "*.stbl.json",
				"url": "./schemas/stbl.schema.json"
			},
			{
				"fileMatch": "s4tk.config.json",
				"url": "./schemas/s4tk-config.schema.json"
			}
		],
		"languages": [
			{
				"id": "json",
				"aliases": [
					"STBL JSON"
				],
				"filenamePatterns": [
					"*.stbl.json"
				]
			},
			{
				"id": "json",
				"aliases": [
					"S4TK Config"
				],
				"filenamePatterns": [
					"s4tk.config.json"
				]
			}
		]
	}
}
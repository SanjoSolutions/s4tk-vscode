{
  "buildInstructions": {
    "source": "src",
    "destinations": [
      "out"
    ],
    "packages": [
      {
        "filename": "MyPackage",
        "include": [
          "**/*"
        ]
      }
    ]
  },
  "buildSettings": {
    "allowEmptyPackages": false,
    "allowFolderCreation": false,
    "allowMissingSourceFiles": false,
    "allowPackageOverlap": false,
    "generateMissingLocales": true,
    "mergeStringTablesInSamePackage": true,
    "minifyTuning": false,
    "outputBuildSummaryFile": true
  },
  "releaseSettings": {
    "filename": "MyZippedModFiles",
    "otherFilesToInclude": [],
    "overrideDestinations": []
  },
  "workspaceSettings": {
    "defaultLocale": "English",
    "defaultStringTable": "src/strings/default.stbl.json",
    "newStringsToStartOfTable": true,
    "newStringTableJsonType": "object",
    "showCopyConfirmationPopup": true,
    "showStblJsonMetaDataButton": true,
    "showXmlKeyOverrideButtons": true,
    "spacesPerIndent": 2
  }
}
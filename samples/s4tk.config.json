{
  "buildInstructions": {
    "source": "src",
    "destinations": [
      "out"
    ],
    "packages": [
      {
        "filename": "MyPackage",
        "include": [
          "**/*"
        ],
        "exclude": []
      }
    ]
  },
  "buildSettings": {
    "allowEmptyPackages": false,
    "allowFolderCreation": false,
    "allowMissingSourceFiles": false,
    "allowPackageOverlap": false,
    "generateMissingLocales": true,
    "mergeStringTablesInSamePackage": true,
    "outputBuildSummary": "partial"
  },
  "releaseSettings": {
    "filename": "MyZippedModFiles",
    "otherFilesToInclude": [],
    "overrideDestinations": []
  },
  "workspaceSettings": {
    "defaultLocale": "English",
    "defaultStringTable": "src/strings/default.stbl.json",
    "defaultStringTableJsonType": "object",
    "newStringsToStartOfStbl": true,
    "showCopyConfirmationPopup": true,
    "spacesPerIndent": 2
  }
}